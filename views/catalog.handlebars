<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Model</th>
            <th>Stock</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#items}}
        <tr>
            <td>{{name}}</td>
            <td>{{model}}</td>
            <td>{{stock}}</td>
            <td>
                <button type="button" class="btn btn-primary" id="btnReq">Requisitar</button>
            </td>
        </tr>
        {{/items}}
    </tbody>
</table>